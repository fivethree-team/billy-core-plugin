{
    "name": "@fivethree/billy-plugin-core",
    "version": "0.14.4",
    "description": "core plugin",
    "repository": {
        "type": "git",
        "url": "https://github.com/fivethree-team/billy-plugin-core"
    },
    "main": "dist/index.js",
    "scripts": {
        "build": "node_modules/.bin/tsc -p .",
        "start": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
        "refresh": "rm -rf ./node_modules ./package-lock.json && npm install"
    },
    "keywords": [
        "typescript",
        "cli",
        "node",
        "terminal"
    ],
    "author": "garygrossgarten",
    "license": "MIT",
    "devDependencies": {
        "@fivethree/billy-core": "0.14.4",
        "@types/node": "^10.12.18",
        "nodemon": "^1.18.9",
        "ts-node": "^7.0.1",
        "typescript": "^3.2.2"
    },
    "dependencies": {
        "inquirer": "6.3.1",
        "@fivethree/billy-plugin-git": "0.0.2"
    },
    "files": [
        "dist"
    ]
}
